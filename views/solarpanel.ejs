<script type="text/javascript">
function gt_check()
{ 
var a = Number(document.getElementById("txt_akwh").value);
var b = Number(document.getElementById("txt_sunhrs").value);
var percent = Number((document.getElementById('txt_percent').value) / 100);
var c = Math.round(((((a * percent) / 30) / b) / 0.80) * 1000);
var d = Number((c * 12) * 1.55);
var e = Number((c * 12 ) / 445);
var bill = Number(document.getElementById('txt_bill').value);
var sav1 = Number(bill * 12);
var sav15 = Number(bill * 180);
var sav20 = Number(bill * 240);
var houseval = Number(sav1 * 20);
document.getElementById("financial").innerHTML = "<br /><span style=\"padding:5px; width:98%; display:block; position:relative; background-color:#cccccc;\"><strong>Financial Benefits</strong></span>";
document.getElementById("utilize").innerHTML = "<p><strong>If you utilize solar for 100% of your electricity needs, </p></strong>";
document.getElementById("housevalue").innerHTML = "You will add $" + houseval + "in value to your home";
document.getElementById("savings1").innerHTML = "Over a year, you will save" +sav1+ " in electric bills ";
document.getElementById("savings15").innerHTML = "Over a year, you will save" +sav15+ " in electric bills ";
document.getElementById("savings20").innerHTML = "Over a year, you will save" +sav20+ " in electric bills ";
document.getElementById("wattage").innerHTML = "You will need a system producing " + (c / 1000) + " DC KW to cover " +Math.round(percent * 100)+ " % of your monthly power usage";
document.getElementById("environmental").innerHTML = "<span style=\"padding:5px; width:98%; display:block; position:relative; background-color:#cccccc;\"><strong>Environmental Benefits</strong></span><br />";
document.getElementById("trees").innerHTML = "You will eliminate " +Math.round(d)+ " lbs of C02 per year and this is equivalent to planting " +Math.round(e)+ " trees every year</strong><br />" ;	
}



</script>

